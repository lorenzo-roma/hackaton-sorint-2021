{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "start"],
            "console":"integratedTerminal",
            "env":{
                "NODE_ENV":"development"
            }
          },
        {
            "name": "Debug",
            "type": "pwa-chrome",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}"
        }
    ]
}